{
  "compilerOptions": {
    "baseUrl": "src",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/(routes)/dashboard/_components/SideNav.jsx", "src/app/_components/Header.jsx", "components/ui/button.jsx", "src/app/(routes)/dashboard/ngan-sach/_components/CreateBudget.tsx", "src/app/(routes)/dashboard/ngan-sach/page.jsx", "src/app/(routes)/dashboard/ngan-sach/_components/BudgetList.tsx", "src/app/(routes)/dashboard/chi-phi/[id]/page.tsx", "src/app/(routes)/dashboard/chi-phi/_components/AddExpense.jsx", "src/app/(routes)/dashboard/chi-phi/_components/EditBudget.jsx", "src/app/(routes)/dashboard/_components/CardInfo.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
