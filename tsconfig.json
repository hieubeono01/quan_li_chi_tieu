{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/(routes)/dashboard/_components/SideNav.jsx", "app/_components/Header.jsx", "components/ui/button.jsx", "app/(routes)/dashboard/ngan-sach/_components/CreateBudget.tsx", "app/(routes)/dashboard/ngan-sach/page.jsx", "app/(routes)/dashboard/ngan-sach/_components/BudgetList.tsx", "app/(routes)/dashboard/chi-phi/[id]/page.tsx", "app/(routes)/dashboard/chi-phi/_components/AddExpense.jsx", "app/(routes)/dashboard/chi-phi/_components/EditBudget.jsx", "app/(routes)/dashboard/_components/CardInfo.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
